package com.tomato.tools.dataBase.Bean;

import org.litepal.crud.DataSupport;

/**
 *  检测对象信息
 */
public class DetectionObjectInfoBean extends DataSupport {
    private long id;//默认主键
    private String detectionName;//检测对象名称
    private float magneticGradient;//磁场梯度值
    private float stressValue;//应力值
    private int defectLevel;//缺陷等级（0一般缺陷，1严重，2非常严重）
    private String material;//材质
    private String model;//型号
    private String Note;//备注

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getDetectionName() {
        return detectionName;
    }

    public void setDetectionName(String detectionName) {
        this.detectionName = detectionName;
    }

    public float getMagneticGradient() {
        return magneticGradient;
    }

    public void setMagneticGradient(float magneticGradient) {
        this.magneticGradient = magneticGradient;
    }

    public float getStressValue() {
        return stressValue;
    }

    public void setStressValue(float stressValue) {
        this.stressValue = stressValue;
    }

    public int getDefectLevel() {
        return defectLevel;
    }

    public void setDefectLevel(int defectLevel) {
        this.defectLevel = defectLevel;
    }

    public String getMaterial() {
        return material;
    }

    public void setMaterial(String material) {
        this.material = material;
    }

    public String getModel() {
        return model;
    }

    public void setModel(String model) {
        this.model = model;
    }

    public String getNote() {
        return Note;
    }

    public void setNote(String note) {
        Note = note;
    }

}
